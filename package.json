    "patch-package": "6.4.7",
    "prettier": "2.7.1",
    "standard-version": "9.5.0",
    "ts-jest": "29.0.2",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.8.4"
